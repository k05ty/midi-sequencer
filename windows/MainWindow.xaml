<Window x:Class="midi_sequencer.windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:midi_sequencer"
        mc:Ignorable="d"
        Title="MainWindow" Height="200" Width="300">
    <Grid x:Name="Grid">
        <Button x:Name="pianoRollButton" Content="Piano roll" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Click="pianoRollButton_Click"/>
        <Button x:Name="playbackButton" Content="Playback" HorizontalAlignment="Left" Margin="10,35,0,0" VerticalAlignment="Top" Width="54" Click="playbackButton_Click"/>
        <Button x:Name="generalButton" Content="General" HorizontalAlignment="Left" Margin="10,60,0,0" VerticalAlignment="Top" Width="54" Click="generalButton_Click"/>
        <Image Margin="158,42,42,42" Source="/playback/icon.png"/>

    </Grid>
</Window>
